{
	"autoload": {
		"psr-4": {
			"Laz0r\\Emitter\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Laz0r\\EmitterTest\\": "test/"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": false
		}
	},
	"description": "PSR-7 Response emitter interface and implementation",
	"keywords": [
		"Laz0r",
		"PSR-7",
		"emitter",
		"psr7",
		"response"
	],
	"license": "MIT",
	"name": "laz0r/emitter",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/laz0rs/emitter"
		}
	],
	"require": {
		"laz0r/util": "^1.1.0",
		"php": "^7.4 || ~8.0.0",
		"psr/http-message": "^1.0.0"
	},
	"require-dev": {
		"fig-r/psr2r-sniffer": "^1.3",
		"phpunit/phpunit": "9.5.16",
		"vimeo/psalm": "^4.26"
	},
	"type": "library"
}
